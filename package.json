  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@rollup/plugin-json": "^4.0.3",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.37",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "relcov": "^1.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
